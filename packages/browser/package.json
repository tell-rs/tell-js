{
  "name": "@tell-rs/browser",
  "version": "0.2.6",
  "description": "Tell SDK for browsers â€” analytics events and structured logging",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js",
      "require": "./dist/events.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "node --import tsx --test test/*.test.ts",
    "test:e2e": "TELL_E2E=1 node --import tsx --test test/e2e.test.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@tell-rs/core": "*",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tell-rs/tell-js.git",
    "directory": "packages/browser"
  },
  "homepage": "https://tell.rs",
  "keywords": [
    "analytics",
    "logging",
    "telemetry",
    "events",
    "observability",
    "tell",
    "browser"
  ],
  "license": "MIT",
  "author": "Arcade Labs Inc."
}
